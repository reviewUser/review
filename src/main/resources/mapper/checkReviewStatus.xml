<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.chinasoft.dao.CheckReviewDao">

    <resultMap type="com.chinasoft.po.CheckReview" id="CheckReviewMap">
        <id column="id" property="id"/>
        <result column="review" property="review"/>
        <result column="phone" property="phone"/>
        <result column="status" property="status"/>
    </resultMap>

    <insert id="insert" parameterType="com.chinasoft.po.CheckReview">
        <selectKey resultType="java.lang.Long" keyProperty="id"
                   order="AFTER">
            SELECT LAST_INSERT_ID()
        </selectKey>
        insert INTO check_review_status(id,review, phone, status)
        VALUES (#{id}, #{review}, #{phone}, #{status})
    </insert>
</mapper>